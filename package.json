{
  "name": "api-auth",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=12.9.1"
  },
  "scripts": {
    "lint": "prettier --write */*.js",
    "start": "node .",
    "start:dev": "node-dev .",
    "build": "node-pg-migrate up && node scripts/generate-keys.js && bash -c 'cp -r node_modules/@gouvfr/dsfr/dist/{dsfr.min.css,fonts,icons,utility} public/'",
    "import": "node scripts/import-babel-wrapper.js",
    "update-organization-info": "node scripts/update-organizations-info-babel-wrapper.js",
    "migrate": "node-pg-migrate",
    "load-fixtures": "psql -d api-auth -f scripts/fixtures.sql && npm run update-organization-info 2000",
    "generate-keystore": "node scripts/generate-keys.js",
    "test:lint": "prettier */*.js --list-different",
    "test": "mocha --require babel-polyfill --require babel-register"
  },
  "license": "MIT",
  "dependencies": {
    "@gouvfr/dsfr": "^1.7.2",
    "@sentry/node": "^7.10.0",
    "@sentry/tracing": "^7.10.0",
    "axios": "^0.27.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "connect-redis": "^6.1.3",
    "csurf": "^1.11.0",
    "csv": "^5.5.3",
    "dotenv": "^8.2.0",
    "ejs": "^2.7.4",
    "email-providers": "^1.13.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "helmet": "^3.23.3",
    "http-errors": "^1.8.1",
    "ioredis": "^4.3.0",
    "lodash": "^4.17.19",
    "morgan": "^1.9.1",
    "nanoid": "^3.0.2",
    "node-pg-migrate": "^5.9.0",
    "oidc-provider": "^6.7.0",
    "pg": "^7.7.1",
    "rate-limiter-flexible": "^0.21.2",
    "tld-extract": "^2.0.1"
  },
  "devDependencies": {
    "cypress-mailslurp": "^1.5.0",
    "mocha": "^7.2.0",
    "node-dev": "^7.0.0",
    "prettier": "^1.15.3"
  }
}
